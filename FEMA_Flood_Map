          { "type": "Feature", "properties": { "ID": 3773, "EPA_ID": "WYD048743009", "Region_1": 8, "Facilty_Na": "SINCLAIR CASPER REFINING COMPANY", "Address": "5700 EAST HIGHWAY 20\/26", "City_1": "CASPER", "State": "WY", "Zip": 82609, "Acreage": 1815, "Extract_Fl": "X", "Federal_Fa": null, "Lat": 42.8581, "Long": -106.2645, "Full_addre": "5700 EAST HIGHWAY 20\/26, CASPER WY, 82609" }, "geometry": { "type": "Point", "coordinates": [ -106.29034963, 42.855181985000058 ] } }
          ]
